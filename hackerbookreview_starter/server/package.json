{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./src/server.js --watch . --exec babel-node",
    "initdb": "npm run createdb && npm run createschema && npm run loaddata",
    "createdb": "cross-env psql -U postgres -f ./database/create_db.sql postgres://database:5432/postgres",
    "createschema": "cross-env psql -U postgres -f ./database/schema.sql postgres://database:5432/hackerbook",
    "loaddata": "cross-env psql -U postgres -f ./database/load_data.sql postgres://database:5432/hackerbook",
    "dropdb": "cross-env psql -U postgres -f ./database/drop_db.sql postgres://database:5432/postgres",
    "dropschema": "cross-env psql -U postgres -f ./database/drop_schema.sql postgres://database:5432/hackerbook",
    "start": "npm run dev"
  },
  "keywords": [],
  "author": "James Moore",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.9.0",
    "axios": "^0.27.2",
    "base-64": "^1.0.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-tools": "^8.3.0",
    "gravatar": "^1.8.2",
    "humps": "^2.0.1",
    "pg": "^8.7.3",
    "ramda": "^0.28.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "2.0.19",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.26.0"
  },
  "type": "module"
}
